{
  "name": "shopify",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "test": "next",
    "start": "NODE_ENV=production node server.js",
    "parcel": "parcel widget/index.html",
    "build:widget": "parcel build --no-source-maps widget/index.html",
    "clean": "rm -rf dist",
    "prod:widget": "yarn clean && yarn build:widget && find ./dist -name '*.js' -exec mv '{}' public/widget.js \\;"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^9.4.0",
    "@shopify/koa-shopify-auth": "^3.1.72",
    "@shopify/polaris": "^5.8.2",
    "@zeit/next-css": "^1.0.1",
    "airtable": "^0.10.0",
    "dotenv": "^8.2.0",
    "isomorphic-fetch": "^3.0.0",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-session": "^6.1.0",
    "koa-static": "^5.0.0",
    "next": "^9.5.5",
    "parcel-bundler": "^1.12.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
